{{#each emails}}
  <div class='card'>
    <div class='card-body'>
      <h5 class='card-title'>
        {{author}}
      </h5>
      <h6 class='card-subtitle mb-2 text-body-secondary'>
        {{category}}
      </h6>
      <p class='card-text'>
        {{description}}
      </p>
      <a href='{{link}}' class='card-link'>
        Read more
      </a>
    </div>
  </div>
{{/each}}